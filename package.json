{
  "name": "eslint-import-resolver-babel-namespace",
  "version": "0.0.8",
  "description": "A resolver plugin for eslint-plugin-import to support babel-plugin-namespace",
  "main": "lib/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint src tests",
    "compile": "babel src --out-dir lib",
    "prepublish": "npm run compile",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:./tests/babel-setup.js --reporter spec tests",
    "test-debug": "DEBUG=babel:plugin:*,eslint:plugin:* mocha --compilers js:./tests/babel-setup.js --reporter spec tests -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yudhasetiawan/eslint-import-resolver-babel-namespace.git"
  },
  "keywords": [
    "babel",
    "eslint",
    "plugin",
    "namespace",
    "import"
  ],
  "author": {
    "name": "Yudha Setiawan",
    "email": "me@yudhasetiawan.com",
    "url": "http://yudhasetiawan.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yudhasetiawan/eslint-import-resolver-babel-namespace/issues"
  },
  "homepage": "https://github.com/yudhasetiawan/eslint-import-resolver-babel-namespace#readme",
  "dependencies": {
    "babel-plugin-namespace": "^0.1.0",
    "debug": "^2.2.0",
    "eslint-import-resolver-node": "^0.2.0",
    "json5": "^0.5.0",
    "lodash": "^4.11.2",
    "path-is-inside": "^1.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.6.1",
    "mocha": "^2.4.5"
  }
}
